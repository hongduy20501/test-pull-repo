{
  "name": "@elofun/eloplay-gamehub",
  "version": "0.0.10-k",
  "description": "",
  "scripts": {
    "start:prod": "node lib/index.js",
    "docker:release": "npm run docker:build && npm run docker:push",
    "docker:build": "docker-compose -f build.yml build",
    "docker:push": "docker-compose -f build.yml push"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elofun/node-utils": "0.0.5-f",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/*.test.ts"
    ],
    "exec": "ts-node -r dotenv/config ./src/index.ts",
    "delay": "2000"
  }
}